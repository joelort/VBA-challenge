Sub getting_tickers()
'lets see if this works
Dim row As Long
row = Cells(Rows.Count, "2018").End(xlUp).row
ActiveSheet.Range("A2:A" & row).AdvancedFilter _
Action:=xlFilterCopy, CopyToRange:=ActiveSheet.Range("J2"), _
Unique:=True
End Sub
